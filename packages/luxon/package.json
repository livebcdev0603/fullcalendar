{
  "name": "@fullcalendar/luxon",
  "version": "6.0.0-beta.1",
  "title": "FullCalendar Luxon 1 Plugin",
  "description": "A connector to the Luxon 1 date library",
  "docs": "https://fullcalendar.io/docs/luxon1",
  "main": "dist/.tsc/main.js",
  "types": "dist/.tsc/main.d.ts",
  "browserGlobal": "FullCalendarLuxon",
  "homepage": "https://fullcalendar.io/",
  "bugs": "https://fullcalendar.io/reporting-bugs",
  "repository": {
    "type": "git",
    "url": "https://github.com/fullcalendar/fullcalendar.git",
    "homepage": "https://github.com/fullcalendar/fullcalendar"
  },
  "license": "MIT",
  "author": {
    "name": "Adam Shaw",
    "email": "arshaw@arshaw.com",
    "url": "http://arshaw.com/"
  },
  "type": "module",
  "dependencies": {
    "@fullcalendar/core": "workspace:*",
    "tslib": "^2.1.0"
  },
  "peerDependencies": {
    "luxon": "^1.12.1"
  },
  "devDependencies": {
    "@fullcalendar/workspace-scripts": "workspace:*",
    "luxon": "^1.12.1"
  },
  "scripts": {
    "build": "workspace-scripts pkg:build",
    "clean": "workspace-scripts pkg:clean"
  },
  "buildConfig": {
    "exports": {
      ".": {
        "iife": {
          "name": "FullCalendarLuxon",
          "globals": {
            "@fullcalendar/core": "FullCalendar",
            "@fullcalendar/core/preact": "FullCalendar",
            "luxon": "luxon"
          }
        }
      }
    }
  },
  "publishConfig": {
    "directory": "./dist",
    "linkDirectory": true
  }
}
