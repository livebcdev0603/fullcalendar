{
  "name": "@fullcalendar/core",
  "version": "6.0.0-beta.1",
  "title": "FullCalendar Core",
  "description": "core package",
  "docs": "https://fullcalendar.io/docs",
  "homepage": "https://fullcalendar.io/",
  "bugs": "https://fullcalendar.io/reporting-bugs",
  "repository": {
    "type": "git",
    "url": "https://github.com/fullcalendar/fullcalendar.git",
    "homepage": "https://github.com/fullcalendar/fullcalendar"
  },
  "license": "MIT",
  "author": {
    "name": "Adam Shaw",
    "email": "arshaw@arshaw.com",
    "url": "http://arshaw.com/"
  },
  "type": "module",
  "dependencies": {
    "preact": "^10.0.5",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@fullcalendar/workspace-scripts": "workspace:*",
    "handlebars": "^4.1.2"
  },
  "scripts": {
    "preflight": "workspace-scripts pkg:preflight",
    "tsc": "workspace-scripts-tsc -b",
    "build": "workspace-scripts pkg:build",
    "dev": "workspace-scripts pkg:build --dev",
    "clean": "workspace-scripts pkg:clean"
  },
  "fileExports": {
    ".": "./src/index.ts",
    "./preact": "./src/preact/index.ts",
    "./preact/compat": "./src/preact/compat.ts",
    "./locales/*": "./src/locales/*.ts"
  },
  "generatedExports": {
    "./locales-all": "./scripts/generate-locales-all.js"
  },
  "iife": {
    ".": "FullCalendar",
    "./locales-all": "FullCalendarLocales"
  },
  "publishConfig": {
    "directory": "./dist",
    "linkDirectory": true
  }
}
