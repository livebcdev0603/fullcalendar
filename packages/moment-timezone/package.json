{
  "name": "@fullcalendar/moment-timezone",
  "version": "6.0.0-beta.1",
  "title": "FullCalendar Moment Timezone Plugin",
  "description": "A connector to the moment-timezone library",
  "docs": "https://fullcalendar.io/docs/moment-timezone-plugin",
  "homepage": "https://fullcalendar.io/",
  "bugs": "https://fullcalendar.io/reporting-bugs",
  "repository": {
    "type": "git",
    "url": "https://github.com/fullcalendar/fullcalendar.git",
    "homepage": "https://github.com/fullcalendar/fullcalendar"
  },
  "license": "MIT",
  "author": {
    "name": "Adam Shaw",
    "email": "arshaw@arshaw.com",
    "url": "http://arshaw.com/"
  },
  "type": "module",
  "dependencies": {
    "@fullcalendar/core": "workspace:*",
    "tslib": "^2.1.0"
  },
  "peerDependencies": {
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.31"
  },
  "devDependencies": {
    "@fullcalendar/workspace-scripts": "workspace:*",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.31"
  },
  "scripts": {
    "build": "workspace-scripts pkg:build",
    "clean": "workspace-scripts pkg:clean"
  },
  "buildConfig": {
    "exports": {
      ".": {
        "iife": {
          "name": "FullCalendarMomentTimezone",
          "globals": {
            "@fullcalendar/core": "FullCalendar",
            "@fullcalendar/core/preact": "FullCalendar",
            "moment": "moment",
            "moment-timezone": "moment-timezone"
          }
        }
      }
    }
  },
  "publishConfig": {
    "directory": "./dist",
    "linkDirectory": true
  }
}
