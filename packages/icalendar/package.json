{
  "name": "@fullcalendar/icalendar",
  "version": "6.0.0-beta.1",
  "title": "FullCalendar iCalendar Plugin",
  "description": "Fetch events from a public iCalendar / .ics feed",
  "docs": "https://fullcalendar.io/docs/icalendar",
  "homepage": "https://fullcalendar.io/",
  "bugs": "https://fullcalendar.io/reporting-bugs",
  "repository": {
    "type": "git",
    "url": "https://github.com/fullcalendar/fullcalendar.git",
    "homepage": "https://github.com/fullcalendar/fullcalendar"
  },
  "license": "MIT",
  "author": {
    "name": "NJ Pearman",
    "email": "n.pearman@gmail.com",
    "url": "https://github.com/njpearman/"
  },
  "type": "module",
  "dependencies": {
    "@fullcalendar/core": "workspace:*",
    "ical.js": "^1.4.0",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@fullcalendar/workspace-scripts": "workspace:*"
  },
  "scripts": {
    "build": "workspace-scripts pkg:build",
    "clean": "workspace-scripts pkg:clean"
  },
  "buildConfig": {
    "exports": {
      ".": {
        "iife": {
          "name": "FullCalendarICalendar",
          "globals": {
            "@fullcalendar/core": "FullCalendar",
            "@fullcalendar/core/preact": "FullCalendar",
            "ical.js": "ICAL"
          }
        }
      }
    }
  },
  "publishConfig": {
    "directory": "./dist",
    "linkDirectory": true
  }
}
